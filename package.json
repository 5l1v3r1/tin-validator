{
  "name": "tin",
  "version": "0.0.1",
  "description": "Taxpayer Identification Number",
  "main": "dist/index.js",
  "homepage": "https://github.com/seegno/tin",
  "bugs": "https://github.com/seegno/tin/issues",
  "repository": "seegno/tin",
  "author": "Seegno",
  "license": "MIT",
  "keywords": [
    "EIN",
    "IRS",
    "ITIN",
    "SSN",
    "TIN"
  ],
  "options": {
    "mocha": "--compilers js:babel-register --recursive --require should test"
  },
  "scripts": {
    "build": "rm -rf dist/* && babel --copy-files src/ --out-dir dist/",
    "changelog": "github_changelog_generator --no-issues",
    "lint": "eslint src test && jscs src test",
    "prepublish": "npm run lint && npm test && npm run build",
    "test": "NODE_ENV=test mocha $npm_package_options_mocha"
  },
  "dependencies": {
    "is-valid-ein": "seegno/is-valid-ein#0.0.2",
    "is-valid-itin": "seegno/is-valid-itin#0.0.2",
    "is-valid-ssn": "seegno/is-valid-ssn#0.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-eslint": "5.0.0-beta9",
    "babel-preset-es2015": "6.5.0",
    "babel-register": "6.4.3",
    "eslint": "1.8.0",
    "eslint-config-seegno": "2.0.0",
    "eslint-plugin-babel": "3.1.0",
    "jscs": "2.9.0",
    "jscs-config-seegno": "1.1.0",
    "mocha": "2.4.5",
    "should": "8.2.1",
    "sinon": "^1.17.3"
  }
}
